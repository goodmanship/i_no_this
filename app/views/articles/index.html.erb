<div class='row'>
  <div class='large-2'></div>
  <div class='large-8'>
    <h1>Search</h1>
    <%= form_tag( '/search', method: :get ) do %>
      <p>
        <%= text_field :search, params[:search] %>
        <%= submit_tag "Search", name: nil %>
      </p>
    <% end %>
  </div>
  <div class='large-2'></div>
</div>

